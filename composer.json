{
    "name": "taluu/doctrine-orm-provider",
    "description": "Doctrine ORM provider for Silex",
    "keywords": ["silex", "doctrine", "orm", "database", "extension", "provider"],
    "homepage": "https://github.com/taluu/nutwerk-orm-extension",
    "license": "MIT",
    
    "authors": [
        { "name": "Marc Jakubowski", "homepage": "http://nutwerk.de/" },
        { "name": "Baptiste \"Talus\" ClaviÃ©", "homepage": "http://www.talus-works.net" }
    ],

    "require": {
        "php": ">=5.3.2",
        "doctrine/orm": ">= 2.3.0, < 2.4.0-beta",
        "doctrine/common": ">= 2.3.0, < 2.4.0-beta",
        "doctrine/dbal": ">= 2.3.0, < 2.4.0-beta",
        "silex/silex": "1.1.*"
    },

    "suggest": {
        "symfony/console": "Necessary if you would like to use the Doctrine CLI tools"
    },

    "autoload": {
        "psr-0": { "Nutwerk\\Provider": "lib/" }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
